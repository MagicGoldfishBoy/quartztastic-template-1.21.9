{
  "multipart": [
    {
      "apply": {
        "model": "quartztastic:block/smoky_quartz_pane_post_ends"
      }
    },
    {
      "apply": {
        "model": "quartztastic:block/smoky_quartz_pane_post"
      },
      "when": {
        "east": "false",
        "north": "false",
        "south": "false",
        "west": "false"
      }
    },
    {
      "apply": {
        "model": "quartztastic:block/smoky_quartz_pane_cap"
      },
      "when": {
        "east": "false",
        "north": "true",
        "south": "false",
        "west": "false"
      }
    },
    {
      "apply": {
        "model": "quartztastic:block/smoky_quartz_pane_cap",
        "y": 90
      },
      "when": {
        "east": "true",
        "north": "false",
        "south": "false",
        "west": "false"
      }
    },
    {
      "apply": {
        "model": "quartztastic:block/smoky_quartz_pane_cap_alt"
      },
      "when": {
        "east": "false",
        "north": "false",
        "south": "true",
        "west": "false"
      }
    },
    {
      "apply": {
        "model": "quartztastic:block/smoky_quartz_pane_cap_alt",
        "y": 90
      },
      "when": {
        "east": "false",
        "north": "false",
        "south": "false",
        "west": "true"
      }
    },
    {
      "apply": {
        "model": "quartztastic:block/smoky_quartz_pane_side"
      },
      "when": {
        "north": "true"
      }
    },
    {
      "apply": {
        "model": "quartztastic:block/smoky_quartz_pane_side",
        "y": 90
      },
      "when": {
        "east": "true"
      }
    },
    {
      "apply": {
        "model": "quartztastic:block/smoky_quartz_pane_side_alt"
      },
      "when": {
        "south": "true"
      }
    },
    {
      "apply": {
        "model": "quartztastic:block/smoky_quartz_pane_side_alt",
        "y": 90
      },
      "when": {
        "west": "true"
      }
    }
  ]
}